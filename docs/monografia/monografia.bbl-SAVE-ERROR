    \entry{dimen_hubs}{article}{}{}
      \name{author}{3}{}{%
        {{un=0,uniquepart=base,hash=c9ca6c3a3b863037975071923b37465a}{%
           family={Radovanović},
           familyi={R\bibinitperiod},
